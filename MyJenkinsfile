pipeline {
  agent any
  stages {
    stage('SCM Checkout') {
       steps {
         git 'https://github.com/vaheedgithubac/Dummy-myweb'
       } 
    } // SCM Checkout
    
     stage('Maven Build') {
        steps {
           def mvnHome=tool name: 'Maven-363', type: 'maven'
           def mvnCmd="${mvnHome}/bin"
           sh label: '', script: 'mvnCmd clean install package'
        } //Maven Build
     }
 }
}
