def mvnHome 
def mvnCmd
pipeline {
  agent any
  options{
        timestamps()
        }
  stages {
    stage('SCM Checkout') {
       steps {
         git 'https://github.com/vaheedgithubac/Dummy-myweb'
       } 
    } // SCM Checkout
    
     stage('Maven Build') {
        steps {
           script {
           mvnHome=tool name: 'Maven-363', type: 'maven'
           mvnCmd="${mvnHome}/bin/mvn"
           //sh "${mvnCmd} clean install package"
           }
           //mvnCmd="${mvnHome}/bin/mvn"
           sh "${mvnCmd} clean install package"
        } //Maven Build
     }
 }
}
